[alias]
  b = branch
  ci = ci-status
  co = checkout
  d = diff --color-words
  dlc = diff --cached HEAD^
  dw = diff --color-words --word-diff -w
  f = fetch
  l = log --abbrev-commit --color --graph --pretty=format:"%Cred%h%Creset\\ -%C(yellow)%d%Creset\\ %s\\ %Cgreen(%cr)%C(bold\\ blue)\\ <%an>%Creset"
  m = merge
  mypulls = browse -- pulls/adamyonk
  pl = pull
  ps = push
  s = status --short
  safe = !mkdir -p .git/safe && echo "Repository marked safe!"
  se = "!git checkout $(git branch | sed 's/*/ /' | selecta)"
  standup = log --author=\"Adam Jahnke\" --pretty=oneline --since yesterday
  sub = submodule
  suba = submodule add
  subf = submodule foreach
  subu = submodule foreach git pull origin master
  wci = watch-ci
  wip = "!$EDITOR $(git ls-files -m)"
  who = blame

[branch]
  autosetuprebase = always

[credential]
  helper = envvar

[color]
  branch = auto
  diff = auto
  status = auto
  ui = 1

[commit]
	#gpgsign = true

[core]
  editor = nvim
  excludesfile = ~/.gitignore
  quotepath = false

[fetch]
  prune = true

[gh]
  protocol = https

[ghi]
	token = !security find-internet-password -a adamyonk -s github.com -l 'ghi token' -w

[github]
  user = adamyonk

[gpg]
  program = gpg

[help]
  autocorrect = 1

[heroku]
  account = me

[hub]
  protocol = https

[include]
  path = ~/.gitconfig.local

[init]
  templatedir = ~/.git_template

[merge]
  ff = false

[mergetool]
  keepbackup = false

[pull]
  rebase = true

[push]
  default = tracking

[rebase]
  autosquash = true

[rerere]
  enabled = 1

[user]
  email = adamyonk@me.com
  name = Adam Jahnke

[url "https://github"]
	insteadOf = git://github

[url "https://github.com/"]
	insteadOf = git@github.com:
